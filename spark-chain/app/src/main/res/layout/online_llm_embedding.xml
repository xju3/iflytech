<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <TextView
        android:id="@+id/online_llm_embedding_notification"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="请点击对应的按钮体验功能。" />
    <Button
        android:id="@+id/online_llm_embedding_startEmbeddingP"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="开始EmbeddingP测试"/>
    <Button
        android:id="@+id/online_llm_embedding_startEmbeddingQ"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="开始EmbeddingQ测试"/>
</LinearLayout>